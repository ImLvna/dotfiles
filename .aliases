alias inst="sudo apt install -y"
alias aliases="vi ~/.aliases && source ~/.aliases"
alias tmp="cd $(mktemp -d)"
alias temp="tmp"
alias tmpdone="TEMPDONE=$(pwd) && cd ~ && rm -rf $TEMPDONE"
alias ssl="sudo certbot"
alias edservice="sudo systemctl daemon-reload && sudo systemctl restart"
alias rmtssh="ssh mobile@127.0.0.1 -p 2200"
alias rmtfs="sshfs root@127.0.0.1:/ ~/iphone -p 2200"
alias hist="cat ~/.zsh_history | grep"
alias makeprod="sudo rm -rf packages && mkdir packages && make clean package FINALPACKAGE=1"
alias pushrepo='mkdir -p ~/Silica/Packages/$(basename $(pwd)) && rm -f ~/Silica/Packages/$(basename $(pwd))/*.deb; cp packages/*.deb ~/Silica/Packages/$(basename $(pwd))/'
alias makerepo="makeprod && pushrepo"
alias code='echo "http://code.lvna.me/?folder=$(pwd)"'
